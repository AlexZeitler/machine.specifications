${spec.Name}

<test if="spec.Status == Status.Failing">
  <div class="failure">&lArr; FAILED</div>
  <p class="exception_type">
    ${spec.Exception.TypeName}
    <pre class="exception_message">
      result.Exception
     </pre>
  </p>
</test>

<div if="spec.Status == Status.NotImplemented" class="notimplemented">&lArr; NOT IMPLEMENTED</div>